import java.util.*;
class Program
{
public static void main(String args[])
{
int num1,num2,result;
Scanner input=new Scanner(System.in);
System.out.println("enter the number 1");
num1=input.nextInt();
System.out.println("enter the number 2");
num2=input.nextInt();
input.close();
 
   result=num1+num2;
System.out.println(" 3+6=9");
}
}
