import java.util.*;
class Program
{
public static void main(String args[])
{
int num1,num2;
Scanner input=new Scanner(System.in);
System.out.println("enter the number of num1");
num1=input.nextInt();
System.out.println("enter the number of num2");
num2=input.nextInt();
input.close();
System.out.printf("Before swapping num1=%d and num2 =%d\n",num1,num2);
temp= num1;
num1= num2;
num2= temp;
System.out.printf("Before swapping num1=%d and num2 =%d\n",num1,num2);
}
}
